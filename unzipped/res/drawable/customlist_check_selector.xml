<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding android:left="-3.0dip" android:top="-3.0dip" android:right="-3.0dip" android:bottom="-3.0dip" />
            <solid android:color="@color/md_white_1000_87" />
        </shape>
    </item>
    <item>
        <selector>
            <item android:state_checked="false" android:drawable="@drawable/checkbox_blank_circle" />
            <item android:state_checked="true" android:drawable="@drawable/ic_check_circle" />
            <item android:drawable="@drawable/checkbox_blank_circle" />
        </selector>
    </item>
</layer-list>
